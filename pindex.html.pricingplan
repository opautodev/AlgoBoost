  <script>
    // vv Remove if you don't want bar animation on banner
    let offset = 0;
    function genCandle(dir, speed, len) {
      const candle = document.createElement("div");
      const body = document.createElement("div");
      const wick = document.createElement("div");

      candle.classList.add("candle");
      wick.classList.add("wick");
      body.classList.add("body");
    
      if (dir >= offset) {
        wick.classList.add("green");
        wick.style.height = len + "px";
        body.classList.add("origin-bottom", "green");
        candle.style.bottom = "+".concat(offset).concat("px");
      } else {
        wick.classList.add("red");
        wick.style.height = len + "px";
        body.classList.add("origin-top", "red");
        offset -= Math.random() * 10;
        candle.style.top = "-".concat(offset).concat("px");
      }

      candle.appendChild(body);
      candle.appendChild(wick);

      return candle;
    }

    function genChart() {
      const chart = document.querySelector("#chart");
      for (let i = 0; i < 28; i++) {
        const nextOffset = (Math.random() > 0.5 ? offset + Math.random() * 50 : offset - Math.random() * 50);
        if (i == 0) continue;
        chart.appendChild(
          genCandle(
            nextOffset,
            Math.random(),
            Math.random() * 100
          )
        );
        offset = nextOffset;
      }
    }
    // ^^ Remove if you don't want bar animation on banner

    window.addEventListener("load", () => {
      const navlinks = document.querySelector("#nav-links");
      for (let i = 0; i < navlinks.children.length; i++) {
        const child = navlinks.children[i];
        if (location.pathname.includes(child.getAttribute("data-location"))) {
          child.classList.toggle("active");
        }
      }

      document.querySelectorAll('a[href^="#"]').forEach(anchor => {
        anchor.addEventListener('click', function (e) {
          e.preventDefault();
          document.querySelector(this.getAttribute('href')).scrollIntoView({
            behavior: 'smooth'
          });
        });
      });

      // Remove if you don't want bar animation on banner
      genChart();
      // Remove if you don't want bar animation on banner
    });
  </script>
